CHIP Mux 
{
  IN x, y, z; 
  OUT f;

  PARTS:
  Not(in=x, out=notx);
  Not(in=y, out=noty);
  Not(in=z, out=notz);
  Or(a=notx, b=noty, out=notxy);
  And(a=notxy, b=notz, out=f);
}

CHIP Mux 
{
  IN x, y, z;
  OUT f;

  PARTS:
  Not(in=x, out=notx);
  Xor(a=y, b=z, out=zxory);
  And(a=zxory, b=notx, out=notxand);
  And(a=x, b=y, out=xy);
  Or(a=notxand, b=xy, out=f);
}
